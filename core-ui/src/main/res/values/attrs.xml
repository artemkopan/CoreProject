<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressButtonView">
        <attr name="pbv_progressColor" format="color" />
        <attr name="pbv_progressSize" format="dimension" />
        <attr name="pbv_progressBorderWidth" format="dimension" />
        <attr name="pbv_progressPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="cpv_strokeWidth" format="dimension" />
        <attr name="cpv_maxProgress" format="integer" />
        <attr name="cpv_startAngle" format="integer" />
        <attr name="cpv_backgroundColor" format="color" />
        <attr name="cpv_foregroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_canExpand" format="boolean" />
        <attr name="el_startExpanded" format="boolean" />
    </declare-styleable>

</resources>
